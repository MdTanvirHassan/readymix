Latest Database Updated for Aktl Inventory


ALTER TABLE `tbl_consumption_info` ADD `dept_id` INT(11) NULL AFTER `sr_no`;
ALTER TABLE `budget_details` ADD `purchase_by` VARCHAR(30) NULL AFTER `brand_id`;
ALTER TABLE `budget_details` CHANGE `purchase_by` `purchase_by` VARCHAR(30) CHARACTER SET latin1 COLLATE latin1_swedish_ci NULL DEFAULT 'Procurement';
ALTER TABLE `tbl_money_indent_details` ADD `purchase_by` VARCHAR(50) NULL AFTER `item_description`;
ALTER TABLE `tbl_money_indent_details` CHANGE `purchase_by` `purchase_by` VARCHAR(50) CHARACTER SET latin1 COLLATE latin1_swedish_ci NULL DEFAULT 'Procurement';



ALTER TABLE `issue_return_code` ADD `branch_id` INT(11) NULL AFTER `ir_code`; 
ALTER TABLE `issue_return` ADD `memo_no` VARCHAR(255) NULL AFTER `created`; 
ALTER TABLE `issue_return` CHANGE `ir_status` `ir_status` ENUM('not received','Received','Pending') CHARACTER SET latin1 COLLATE latin1_swedish_ci NULL DEFAULT 'Pending'; 

ALTER TABLE `issue_return` ADD `branch_id` INT(11) NULL AFTER `memo_no`, ADD `dept_id` INT(11) NULL AFTER `branch_id`, ADD `created_by` INT(11) NULL AFTER `dept_id`, ADD `updated_by` INT(11) NULL AFTER `created_by`, ADD `updated_date` DATE NULL AFTER `updated_by`;
ALTER TABLE `issue_return` ADD `memo_or_sr_date` DATE NULL AFTER `updated_date`;  
ALTER TABLE `issue_return` CHANGE `memo_no` `memo_or_sr_no` VARCHAR(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci NULL DEFAULT NULL; 
ALTER TABLE `issue_return` ADD `received_by` VARCHAR(50) NULL DEFAULT 'Memo' AFTER `memo_or_sr_date`; 


